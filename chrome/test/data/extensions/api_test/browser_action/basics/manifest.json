{
  "name": "A browser action with no icon that makes the page red",
  "version": "1.0",
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*",
    "experimental", "incognito"
  ],
  "browser_action": {
    "default_title": "Make this page red",
    "default_icon": "icon.png"
  }
}
