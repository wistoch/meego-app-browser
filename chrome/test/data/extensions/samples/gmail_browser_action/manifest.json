{
  "name": "Gmail Checker 2",
  "description": "This extension adds a Gmail button to the toolbar which notifies when new email comes",
  "version": "1.0",
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*.google.com/", "https://*.google.com/"
   ],
  "browser_action": {
      "name": "Gmail",
      "icons": ["blank.png"]
  },
  "icons": {
    "128": "icon_128.png"
  }
}
