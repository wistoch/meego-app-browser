	Name="Output RLZ copy"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="Output RLZ copy"
			CommandLine="$(SolutionDir)..\rlz\copy_files.bat $(InputPath) $(OutDir)"
