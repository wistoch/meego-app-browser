Import('env')

env = env.Clone()

env.ApplySConscript([
    '$BSPATCH_DIR/using_bspatch.scons',
])

if env['PLATFORM'] == 'win32':
  env.Append(
    CCFLAGS = [
        '/TP',
        '/wd4800',
    ],
  )

input_files = [
    "mbsdiff.cc",
]

env.ChromeStaticLibrary('bsdiff', input_files)
